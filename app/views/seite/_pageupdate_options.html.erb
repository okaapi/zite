
<div class="card">
  <div class="card-header" >
    <button class="btn btn-primary" type="button" data-bs-toggle="collapse"
	    data-bs-target="#collapseVersions" aria-expanded="false"
	    aria-controls="collapseVersions">
      Versions
    </button>

    <div id="collapseVersions" class="collapse" >
      <div><br></div>
      <div class="card">
        <div class="card-body">      
          <%= render partial: "versions" %>
        </div>   
      </div>
    </div>   
  </div>
</div>  

<div class="card">
  <div class="card-header" >
    <button class="btn btn-primary" type="button" data-bs-toggle="collapse"
	    data-bs-target="#collapseFiles" aria-expanded="false"
	    aria-controls="collapseFiles">
      File Uploads
    </button>

    <div id="collapseFiles" class="collapse" >
      <div><br></div>
      <div class="card">
        <div class="card-body">      
          <%= render partial: "files", locals: { page: @page } %>  
        </div>   
      </div>
    </div>   
  </div>
</div>  
